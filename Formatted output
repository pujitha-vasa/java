import java.io.*;
public class Main
{
    public static void main(String args[])
    {
        String str="PUJITHA";
        System.out.printf("The string value is:%s\n",str);
        int x=512;
        System.out.printf("\nThe integer valueis:%s\n",x);
        double f=5.25412368;
        System.out.printf("The decimal value is:%f\n",f);
        System.out.printf("\nAfter formatting two specific width of the value is:%4f\n",f);
        System.out.printf("\nAfter formatting to right margin the value is:%20.4f\n",f);
        
    }
}

