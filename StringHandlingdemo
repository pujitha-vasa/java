import java.io.*;
import java.lang.String;
class StringHandlingdemo
{
	public static void main(String args[])
	{
		String s1="Welcome";
		String s2=new String("welcome");
		String s3="Wel";
		boolean x=s1.equals(s2);
		System.out.println("s1 and s2 are equal "+x);
		boolean y=s1.equals(s3);
		System.out.println("s1 and s3 are equal "+y);
		System.out.println("The character at third position in s3 is "+s3.charAt(2));
		System.out.println("The character at fourth position in s1 is "+s1.charAt(4));
		System.out.println("Length of s1 is "+s1.length());
		System.out.println("s1 in uppercase is "+s1.toUpperCase());
		System.out.println("The substring from 2,5 in s1 is "+s1.substring(2,5));7
		System.out.println("The ondex of c in s2 is "+s2.indexOf('c'));
		System.out.println("The after concatenting s1 and s3 is "+s1.concat(s3));
		System.out.println("The after concatenting s1 and s3 is "+s3.concat(s1));
		System.out.println("The s1 contains s3 is "+s1.contains(s3));
		System.out.println("The s3 contains s1 is  "+s3.contains(s1));
	}
}
		
		
